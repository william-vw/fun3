    ] ;
    mf:result [
        qt:data <call_rules-ref.n3>
    ] ;
    rdfs:comment "Call other rules (both terms concrete)" ;
    rdft:approval rdft:Approved ;
    test:options [ test:conclusions "true"^^xsd:boolean ] .


